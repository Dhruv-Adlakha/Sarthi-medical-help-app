   </div>\n      )}\n      {props && props.isAuthenticated && (\n        <div className='navbar'>\n          <h1>\n            <NavLink to='/'>Sarthi</NavLink>\n          </h1>\n          <ul>\n            {(props.currUser.profileVerified === 'Verified' ||\n              props.currUser.role === 'admin') && (\n              <li>\n                <NavLink\n                  className='isHover'\n                  to={`/${props.currUser.role}/dashboard`}\n                >\n                  Dashboard\n                </NavLink>\n              </li>\n            )}\n            <li>\n              <NavLink className='isHover' to='/' onClick={onClickHandler}>\n                Logout\n              </NavLink>\n            </li>\n          </ul>\n        </div>\n      )}\n    </div>\n  );\n};\n\nconst mapStateToProps = (state) => {\n  return {\n    currUser: state.authReducer.currUser,\n    isAuthenticated: state.authReducer.isAuthenticated,\n  };\n};\n\nexport default connect(mapStateToProps)(Navbar);\n","/home/dhruv/Desktop/Sarthi/client/src/components/Layout/LandingPage.js",[],"/home/dhruv/Desktop/Sarthi/client/src/Routers/AppRouter.js",[],"/home/dhruv/Desktop/Sarthi/client/src/components/Authentication/Login.js",[],"/home/dhruv/Desktop/Sarthi/client/src/components/Authentication/Signup.js",[],"/home/dhruv/Desktop/Sarthi/client/src/components/ExtraPages/About.js",[],"/home/dhruv/Desktop/Sarthi/client/src/components/Doctors/DoctorProfiles.js",["216"],"import React, { useEffect } from 'react';\nimport { connect } from 'react-redux';\nimport Navbar from '../Layout/Navbar';\nimport DoctorProfile from '../Doctors/DoctorProfile';\n\nfunction DoctorProfiles(props) {\n  return (\n    <div>\n      <Navbar />\n      <div className='profiles'>\n        <h2>Doctors</h2>\n        <div className='cardsPane'>\n          {props.doctors &&\n            props.doctors.map((doctor, index) => {\n              return <DoctorProfile doctor={doctor} key={index} />;\n            })}\n        </div>\n      </div>\n    </div>\n  );\n}\n\nconst mapStateToProps = (state) => {\n  return {\n    doctors: state.adminReducer.doctors,\n  };\n};\n\nexport default connect(mapStateToProps)(DoctorProfiles);\n","/home/dhruv/Desktop/Sarthi/client/src/components/Doctors/DoctorProfile.js",[],"/home/dhruv/Desktop/Sarthi/client/src/components/Doctors/DoctorRequests.js",["217"],"import React, { useEffect, useState } from 'react';\nimport Navbar from '../Layout/Navbar';\nimport DoctorRequest from '../Doctors/DoctorRequest';\nimport { connect } from 'react-redux';\n\nfunction DoctorRequests(props) {\n  const [drequests, setDrequests] = useState([]);\n  console.log(props.requests);\n  useEffect(() => {\n    setDrequests(() => {\n      return props.requests.filter(\n        (e) => e.applicationStatus === 1 || e.applicationStatus === 2\n      );\n    });\n  }, []);\n  return (\n    <div>\n      <Navbar />\n      <div className='doctorRequests'>\n        <h2>Patient requests</h2>\n        {drequests.map((e) => (\n          <DoctorRequest\n            problem={e.problem}\n            description={e.description}\n            applicationStatus={e.applicationStatus}\n            patient={props.needy.find((e2) => {\n              return e.patient === e2._id;\n            })}\n            id={e._id}\n          />\n        ))}\n      </div>\n    </div>\n  );\n}\n\nconst mapStateToProps = (state) => {\n  return {\n    requests: state.needyReducer.requests,\n    needy: state.adminReducer.needy,\n  };\n};\n\nexport default connect(mapStateToProps)(DoctorRequests);\n","/home/dhruv/Desktop/Sarthi/client/src/components/Doctors/DoctorRequest.js",["218","219"],"import React, { useEffect, useState } from 'react';\nimport { NavLink } from 'react-router-dom';\nimport { connect } from 'react-redux';\nimport { doctorAcceptRequest } from '../../redux/actions/Doctor';\n\nfunction DoctorRequest(props) {\n  const [request, setRequest] = useState('');\n  const [updated, setUpdated] = useState(false);\n  useEffect(() => {\n    setRequest(props.requests.find((e) => e._id === props.id));\n  }, []);\n  const onAcceptHandler = () => {\n    const res = props.dispatch(doctorAcceptRequest(request));\n    if (res) {\n      setUpdated(true);\n    }\n  };\n  return (\n    <div className='doctorRequest'>\n      <h2>Problem: {props.problem}</h2>\n      <h5>Patient: {props.patient && props.patient.name}</h5>\n      <p>{props.description}</p>\n      <div className='buttonSection'>\n        <button\n          className='btn acceptButton'\n          onClick={onAcceptHandler}\n          disabled={props.applicationStatus === 2}\n        >\n          {props.applicationStatus === 2 ? 'Accepted' : 'Accept'}\n        </button>\n        <NavLink to='/doctors/requests/prescribe' className='btn'>\n          Prescribe medicines\n        </NavLink>\n      </div>\n    </div>\n  );\n}\n\nconst mapStateToProps = (state) => {\n  return {\n    requests: state.needyReducer.requests,\n  };\n};\n\nexport default connect(mapStateToProps)(DoctorRequest);\n","/home/dhruv/Desktop/Sarthi/client/src/components/Doctors/DoctorDashboard.js",[],"/home/dhruv/Desktop/Sarthi/client/src/components/Doctors/DoctorMyProfile.js",[],"/home/dhruv/Desktop/Sarthi/client/src/components/Doctors/DoctorUpdateProfile.js",[],"/home/dhruv/Desktop/Sarthi/client/src/components/Doctors/PrescribeMedicines.js",["220"],"import React from 'react';\nimport { NavLink } from 'react-router-dom';\nimport Navbar from '../Layout/Navbar';\n\nfunction PrescribeMedicines() {\n  return (\n    <div>\n      <Navbar />\n      <div className='prescribeMedicines'>\n        <div>\n          <div className='doctorRequest'>\n            <h2>Problem: Fever</h2>\n            <p>\n              I have been suffering from fever for the last 5 days and have\n              stomach ache also.pppppp\n            </p>\n          </div>\n        </div>\n        <h2>Prescription</h2>\n        <div className='addedMedicine'>\n          <p>1. Roxid 40mg</p>\n        </